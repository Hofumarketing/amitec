                <circle cx="8.26953" cy="8.62402" r="6.92285" stroke="black" stroke-width="2"/>
                <line x1="12.2385" y1="14.3609" x2="16.9922" y2="20.6992" stroke="black" stroke-width="2"/>
            </svg>
        </div>
        <p class=" py-12">Popularne wyszukiwania: <b>Hipowit Premiks Trzoda</b></p>
    </div>
</div>

</div>
<script>
    const searchOpen = document.getElementById('search-open');
    const searchClose = document.getElementById('search-close');
    const searchResult = document.getElementById('search_result');

    document.getElementById('search-toggle').addEventListener('click', function(event) {
        event.preventDefault();
        searchOpen.style.display = searchOpen.style.display === 'none' ? 'inline-block' : 'none';
        searchClose.style.display = searchClose.style.display === 'none' ? 'inline-block' : 'none';

        // Toggle class for animation
        if (searchResult.classList.contains('search-open')) {
            searchResult.style.height = '0';
            setTimeout(() => {
                searchResult.classList.remove('search-open');
            }, 300); // Wait for the transition to finish
        } else {
            searchResult.classList.add('search-open');
            searchResult.style.height = '30vh';
        }
    });
</script>
<script>
    document.addEventListener('scroll', function() {
        const nav = document.querySelector('nav');

        if (window.scrollY > 100) {
            nav.classList.add('nav-scrolled');
        } else {
            nav.classList.remove('nav-scrolled');
        }
    });
</script>
